
@{
    ViewBag.Title = "SweetAlert";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
</br>
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script src="~/Scripts/jquery-3.4.1.js"></script>

<button class="btn btn-primary" id="btn1">Buton1</button>
<button class="btn btn-primary" id="btn2">Buton2</button>
<button class="btn btn-primary" id="btn3">Buton3</button>

<script>
    $('#btn1').click(function () {
        swal("Hello world!");
    });

</script>

<script>
    $('#btn2').click(function () {
        swal("Başlık", "Metin");
    });

</script>

<script>
    $('#btn3').click(function () {
        swal("Başlık", "Metin","success");
    });

</script>